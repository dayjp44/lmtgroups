<div class="adminPadding adminFont" style="text-align:left;">
	<div><%= link_to "Attendance Reports", attendance_report_site_path(@facilitator)  %></div>
	<h1>Edit Report</h1>
	<%= flash[:notice ]%>
	<% form_for :report, :url => report_update_site_path(:facilitator_id => @facilitator) do |f| -%>
		<%= render :partial => f %>
		<p><%= submit_tag 'Update' %></p>
	<% end -%>
</div>